#include <iostream>
int main() {
	int N = 0 ;
	std::cin >> N;
	if (N % 10 == 1) {
		std::cout << N << " " << "bochka";
	}
	if (N % 10 == 2 || N % 10 == 3 || N % 10 == 4) {
		std::cout << N << " " << "bochki";
	}
	if ((N % 10 >= 5 && N % 10 <= 9) || N % 10 == 0) {
		std::cout << N << " " << "bochek";
	}
	return EXIT_SUCCESS;
}
