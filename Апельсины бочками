#include <iostream>
int main() {
	int n = 0;
	std::cin >> n;
	if (n % 10 == 1) {
		std::cout << n << " " << "bochka";
	}
	if (n % 10 == 2 || n % 10 == 3 || n % 10 == 4) {
		std::cout << n << " " << "bochki";
	}
	if ((n % 10 >= 5 && n % 10 <= 9) || n % 10 == 0) {
		std::cout << n << " " << "bochek";
	}
	return EXIT_SUCCESS;
}
