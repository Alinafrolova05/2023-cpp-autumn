#include <iostream>
int main() {
	int n ;
	std::cin >> n;
	int a;
	// пятая цифра
	a = n / 60;
	// четвертая цифра
	int b;
	b = (n % 60) / 20;
	// третья цифра
	int c;
	c = ((n % 60) % 20) / 10;
	// вторая цифра
	int d;
	d = (((n % 60) % 20) % 10) / 5;
	// первая цифра
	int e;
	e = ((((n % 60) % 20) % 10));
	std::cout << e << " " << d << " " << c << " " << b << " " << a;
	return EXIT_SUCCESS;
}
